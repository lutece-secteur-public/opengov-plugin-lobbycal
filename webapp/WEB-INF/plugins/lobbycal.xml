<?xml version="1.0" encoding="UTF-8"?>
<plug-in>
    <name>lobbycal</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>1.0.0</version>
    <documentation></documentation>
    <installation></installation>
    <changes></changes>
    <user-guide></user-guide>
    <description>lobbycal.plugin.description</description>
    <provider>lobbycal.plugin.provider</provider>
    <provider-url>http://lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/plugins/lobbycal/lobbycal.png</icon-url>
    <copyright>Copyright (c) 2017 City Of Paris</copyright>
    <db-pool-required>0</db-pool-required>
    
    <core-version-dependency>
        <min-core-version>4.1.0</min-core-version>
        <max-core-version/>	
    </core-version-dependency>

  <!-- Feuilles de style CSS spécifiques éventuelles -->
    <css-stylesheets>
         <css-stylesheet>lobbycal/custom.css</css-stylesheet>
         <css-stylesheet>lobbycal/datatables.min.css</css-stylesheet>
         <css-stylesheet>lobbycal/jquery.datatables.css</css-stylesheet>
    </css-stylesheets>
    
    <!-- Scripts eventuels  -->
    <javascript-files>
    </javascript-files>

    <!-- XPages parameters -->
    <applications>
        <application>
            <application-id>lobbycal</application-id>
            <application-class>fr.paris.lutece.plugins.lobbycal.web.LobbycalApp</application-class>
        </application>
    </applications>

</plug-in>